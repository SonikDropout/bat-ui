{
  "name": "bat-ui",
  "version": "1.5.0",
  "description": "GUI for battery stand",
  "main": "main.js",
  "repository": {
    "url": "https://github.com/SonikDropout/bat-ui",
    "type": "git"
  },
  "scripts": {
    "wp-watch": "cross-env NODE_ENV=development webpack --watch",
    "wp-build": "cross-env NODE_ENV=production webpack",
    "postinstall": "electron-builder install-app-deps",
    "el-watch": "cross-env NODE_ENV=development electron .",
    "el-build": "electron-builder --dir --linux --armv7l",
    "dev": "run-p wp-watch el-watch",
    "inspect": "electron --inspect .",
    "build": "run-s wp-build el-build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chokidar": "^3.3.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "electron": "^9.4.0",
    "electron-builder": "^22.3.2",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^1.1.3",
    "svelte": "^3.18.1",
    "svelte-loader": "^2.13.6",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "chart.js": "^2.9.3",
    "chartjs-plugin-zoom": "^0.7.5",
    "drivelist": "^8.0.9",
    "exceljs": "^4.2.0",
    "serialport": "^8.0.7",
    "usb-detection": "^4.7.0"
  },
  "build": {
    "linux": {
      "executableName": "bat-ui",
      "category": "education"
    }
  },
  "prettier": {
    "svelteSortOrder": "scripts-markup-styles",
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
